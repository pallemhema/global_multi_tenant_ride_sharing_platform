    manufacture_year integer,
    status text DEFAULT 'active'::text,
    created_at_utc timestamp with time zone DEFAULT now(),
    created_by bigint,
    updated_at_utc timestamp with time zone,
    updated_by bigint,
    CONSTRAINT chk_vehicle_owner_consistency CHECK ((((owner_type = 'driver'::text) AND (driver_owner_id IS NOT NULL) AND (fleet_owner_id IS NULL)) OR ((owner_type = 'fleet_owner'::text) AND (fleet_owner_id IS NOT NULL) AND (driver_owner_id IS NULL))))
);


